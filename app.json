{
  "expo": {
    "name": "nhax",
    "slug": "nhax",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nhax-icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sonnv.nhax",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.sonnv.nhax",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "build": {
        "babel": {
          "include": ["static-container"]
        }
      }
    },
    "extra": {
      "test": "prod"
    }
  }
}
